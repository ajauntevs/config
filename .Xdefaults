Xft.dpi:                96
!Xft.rgba:               rgb
!Xft.antialias:          true
!Xft.hinting:            true
!Xft.hintstyle:          hintslight

URxvt.buffered:         true
URxvt.cursorColor:      grey
URxvt.underlineColor:   white

!URxvt.font:             xft:Monospace:pixelsize=12

URxvt.font:             xft:DejaVu Sans Mono-9:Style=Book
URxvt.boldFont:         xft:DejaVu Sans Mono-9:style=Bold

!URxvt.font:             xft:Inconsolata:pixelsize=14
!URxvt.boldFont:         xft:Inconsolata:bold:pixelsize=14

URxvt.scrollBar:        false
URxvt.cursorBlink:      true
URxvt.urlLauncher:      mozilla-minefield
